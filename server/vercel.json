{
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "server/package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "server/**/*"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  